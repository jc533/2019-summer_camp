<html>
    <head>
        <%- include('head'); %>
        <script src="https://unpkg.com/sweetalert2@7.0.6/dist/sweetalert2.all.js"></script>
    </head>
    <body>
        <!-- Wrapper -->
        <div id="wrapper">
            <!-- Header -->
            <%- include('header'); %>
            <!-- Main -->
            <div class="main schedule">
                <div class="inner">
                    <h1>營隊課表</h1>
                    <div class="table-wrapper">
                        <table class="alt">
                            <thead>
                                <tr>
                                    <th>時間</th>
                                    <th>7/24(三)</th>
                                    <th>7/25(四)</th>
                                    <th>7/26(五)</th>
                                    <th>7/27(六)</th>
                                    <th>7/28(日)</th>
                                    <th>7/29(一)</th>
                                    <th>7/30(二)</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr id="place">
                                    <td>集合地點</td>
                                    <td style="font-size:85%">捷運中正紀念堂站</td>
                                    <td style="font-size:85%">捷運松江南京站</td>
                                    <td style="font-size:85%; font-weight:600;">活動開始後公布</td>
                                    <td style="font-size:85%;">捷運七張站</td>
                                    <td style="font-size:85%">捷運七張站</td>
                                    <td style="font-size:85%">捷運松江南京站</td>
                                    <td style="font-size:85%">捷運松江南京站</td>
                                </tr>
                                <tr id="early">
                                    <td>08:00~09:00</td>
                                    <td>破冰時間</td>
                                    <td>晨間早操</td>
                                    <td rowspan="5">神秘活動<br>(至21:30)</td>
                                    <td>晨間早操</td>
                                    <td>晨間早操</td>
                                    <td>晨間早操</td>
                                    <td>晨間早操</td>
                                </tr>
                                <tr id="morning">
                                    <td>0900~12:00</td>
                                    <td>大地遊戲</td>
                                    <td><a id="cpp">C/C++</a></td>
                                    <td><a id="js">Javascript小遊戲</a></td>
                                    <td><a id="bot">discord bot</a></td>
                                    <td><a id="ml">machine learning</a></td>
                                    <td>成果發表</td>
                                </tr>
                                <tr id="lunch">
                                    <td>1200~14:00</td>
                                    <td>午餐時間</td>
                                    <td>午餐時間</td>
                                    <td>午餐時間</td>
                                    <td>午餐時間</td>
                                    <td>午餐時間</td>
                                    <td>午餐時間</td>
                                </tr>
                                <tr id="afternoon">
                                    <td rowspan="2">14:00~17:00</td>
                                    <td rowspan="2"><a id="Photocap">Photocap</a></td>
                                    <td rowspan="2">企業參訪</td>
                                    <td rowspan="2">真人 RPG</td>
                                    <td rowspan="2">桌遊時間</td>
                                    <td rowspan="2"><a id="latex">LaTeX</a></td>
                                    <td rowspan="2">神秘晚會<br>(至21:30)</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <!-- Footer -->
            <%- include('footer'); %>
        </div>

        <!-- Scripts -->
        <script src="assets/js/jquery.min.js"></script>
        <script src="assets/js/skel.min.js"></script>
        <script src="assets/js/util.js"></script>
        <!--[if lte IE 8]><script src="assets/js/ie/respond.min.js"></script><![endif]-->
        <script src="assets/js/main.js"></script>
        <script>
        (function($){
            <% Subject.forEach(function(val, index, arr){ %>
                $("#<%= val.id %>").click(function(){
                    swal({
                    title: '<%= val.title %>',
                    text: '<%= val.text %>',
                    imageUrl: 'images/<%= val.img %>'
                    });
                });
            <% }); %>
        })(jQuery);
        </script>
        <style>
            a {
                cursor: pointer;
            }
        </style>
    </body>
</html>
